<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.phonegap.example"
       
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>TECLADO</name>

    <description>
        utilizacion del pluging de sonidos
    </description>

    
	
    <author href="https://build.phonegap.com" email="jacob.monterrosas@gmail.com">
 JacobMonterrosasDillarza4H
</author>
 <!--personaliza la aplicacion y plataforma -->
 <preference name="orientation" value="landscape"/>
 <!--<preference name="orientation" value="portrait"/>
 <preference name="orientation" value="landscape"/>-->
 <preference name="target-device" value="universal"/>
 <preference name="fullscreen" value="true"/>
 <preference name="android-installLocation" value="auto"/>
 <preference name="permissions" value="INTERNET"/> 

<!--Plugins-->
<!-- Core plugins-->
<plugin name="cordova-plugin-device" spec="~1.1.4"/>
<plugin name="cordova-plugin-vibration" spec="~2.1.3"/>
<plugin name="cordova-plugin-dialogs" spec="~1.3.1"/>	
<plugin name="cordova-plugin-media" spec="~5.0.2" />

<!-- <plugin name="cordova-plugin-geolocation" source="npm"/> -->
<!-- third party plugins-->
<!--para accesar a paginas en internet-->
<allow-intent href="https://*/*"/>
<acces origin="https://*/*"/>
<allow-intent href="http://*/*"/>
<icon src="res/android/mdpi.png"/>
<acces origin="http://*/*"/>
<plugin name="cordova-plugin-whitelist" sourse="npm"/>

<!--colocar icono app-->


		<!-- PARA QUE EL DISPOSITIVO SIEMPRE ESTE DE FORMA HORIZONTAL-->
		
</widget>
